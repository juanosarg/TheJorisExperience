		<stages>
			<li>
				<restFallFactor>0.1</restFallFactor>
				<painFactor>0.01</painFactor>
				<statOffsets>
					<PainShockThreshold>1</PainShockThreshold>
				</statOffsets>
			</li>
		</stages>
	</HediffDef>

	


</Defs>